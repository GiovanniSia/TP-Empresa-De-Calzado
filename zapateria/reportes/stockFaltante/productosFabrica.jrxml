	<queryString>
		<![CDATA[SELECT * FROM zapateria.maestroproductos as mp, zapateria.sucursales as suc 
WHERE mp.Estado LIKE 'Activo'
 AND mp.Tipo LIKE '$P!{TipoProducto}'
 AND  NOT EXISTS (select SUM(sto.StockDisponible) 
 	FROM zapateria.stock as sto where sto.IdSucursal = '$P!{IdSucursal}' AND sto.StockDisponible > 0.0
  AND sto.IdProducto =  mp.IdMaestroProducto GROUP BY IdProducto)
   AND suc.IdSucursal = '$P!{IdSucursal}'
   AND mp.Fabricado LIKE 'S']]>
	</queryString>
