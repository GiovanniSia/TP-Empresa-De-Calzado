	<parameter name="IdSucursal" class="java.lang.Integer"/>
	<parameter name="TipoProducto" class="java.lang.String"/>
	<queryString language="SQL">
		<![CDATA[SELECT *, SUM(st2.StockDisponible) FROM zapateria.maestroproductos as mp
, zapateria.sucursales as suc
, zapateria.stock as st2 
WHERE mp.Estado LIKE 'Activo'
 AND mp.Tipo LIKE '$P!{TipoProducto}'
 AND (mp.PuntoRepositorio*2 >= (select SUM(sto.StockDisponible) 
 	FROM zapateria.stock as sto where sto.IdSucursal = '$P!{IdSucursal}'
 	 AND sto.IdProducto =  mp.IdMaestroProducto AND sto.StockDisponible>0.0 GROUP BY sto.IdProducto) 
 )
  AND suc.IdSucursal = '$P!{IdSucursal}'
   AND st2.IdSucursal = '$P!{IdSucursal}' AND st2.IdProducto = mp.IdMaestroProducto
    AND mp.Fabricado LIKE 'S'
   
    group by mp.IdMaestroProducto]]>
	</queryString>
